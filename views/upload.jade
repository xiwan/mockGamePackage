extends layout

block content
  form(role="form", action="/qrcode/upload", method="post", enctype="multipart/form-data")
    div(class="form-group")
      label(for="username") 用户名       
      input(type="txt", name="username", id="username")
    div(class="form-group")
      label(for="onixfile") 二维码图片       
      input(type="file", name="qrImage", id="qrImage")
    div(class="form-group")       
      input(type="submit", name="提交", id="submit", data-transition="fade")
